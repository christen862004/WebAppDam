@model Department
@{
    ViewData["Title"] = "New";
}

<h1>New</h1>
<!--/Department/SaveNew?Name=&ManagerName=-->
<form method="post" action="/Department/SaveNew">
    <div>
        <label>Name</label>
        <input class="form form-control" type="text" name="Name" id="Name" value="@Model?.Name"/>
    </div>
    <div>
        <label>Manager</label>
        <input class="form form-control" type="text" name="ManagerName" id="ManagerName" value="@Model?.ManagerName" />
    </div>
    <input type="submit" value="Create" class="btn btn-primary"/>
</form>
